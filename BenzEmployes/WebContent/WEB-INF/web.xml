<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>EmployeeData</display-name>
	<welcome-file-list>
		<welcome-file>/jsp_pages/Home.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>EmployDataInserting</servlet-name>
		<servlet-class>servlet.EmployDataInserting</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployDataInserting</servlet-name>
		<url-pattern>/inserting.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EmployDataDisplay</servlet-name>
		<servlet-class>servlet.EmployDataDisplay</servlet-class>

	</servlet>
	<servlet-mapping>

		<servlet-name>EmployDataDisplay</servlet-name>
		<url-pattern>/display.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EmployDataDeleting</servlet-name>
		<servlet-class>servlet.EmployDataDeleting</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployDataDeleting</servlet-name>
		<url-pattern>/delete.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EmployDataFetching</servlet-name>
		<servlet-class>servlet.EmployDataFetching</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployDataFetching</servlet-name>
		<url-pattern>/empfetching.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EmployDataUpdate</servlet-name>
		<servlet-class>servlet.EmployDataUpdate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployDataUpdate</servlet-name>
		<url-pattern>/update.do</url-pattern>
	</servlet-mapping>
</web-app>